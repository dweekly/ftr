{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(cargo fmt:*)",
      "Bash(cargo tarpaulin:*)",
      "Bash(RUST_BACKTRACE=1 cargo test test_global_cache -- --nocapture --test-threads=1)",
      "Bash(cargo fmt:*)",
      "Bash(find:*)",
      "Bash(cargo check:*)",
      "Bash(cargo test:*)",
      "Bash(cargo build:*)",
      "Bash(ssh:*)",
      "Bash(timeout 10s cargo test --test caching_verification_test -- --nocapture)",
      "Bash(echo \"Exit code: $?\")",
      "Bash(gtimeout 10s cargo test --test caching_verification_test -- --nocapture)",
      "Bash(gh release view:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(./target/debug/ftr:*)",
      "WebSearch",
      "WebFetch(domain:www.peeringdb.com)",
      "WebFetch(domain:docs.peeringdb.com)",
      "Bash(~/.cargo/bin/cargo test --lib)",
      "Bash(~/.cargo/bin/cargo test:*)"
    ],
    "deny": []
  }
}